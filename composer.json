{
    "name": "ankane/hightop",
    "description": "A nice shortcut for group count queries with Eloquent / Laravel",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ankane/hightop-php/issues",
        "source": "https://github.com/ankane/hightop-php"
    },
    "authors": [
        {
            "name": "Andrew Kane",
            "email": "andrew@ankane.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hightop\\": "src/"
        }
    },
    "require": {
        "php": ">= 8.1",
        "illuminate/database": ">= 10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "illuminate/database": ">= 10",
        "illuminate/events": ">= 10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hightop\\HightopServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit tests"
    }
}
